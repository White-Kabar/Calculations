package javaapplication68;

public class JavaApplication68 {

    public static void main(String[] args) {
    
    // Creating int variables a, b, c, d, e, f, g.
    int a = 6, b = 5, c = 7, d = 3;
    
    // Variable (int e) will be holding the sum of int a and b.
    int e = a + b;
    
    // Variable (int f) will be holding the subtraction of int c from d. 
    int f = c - d;
    
    // Variable (int g) will be holding the multiplication of (int e) anf (int f).
    g = e * f;
    
    // This line of code is to print out the result of the multiplication of (int e) and (int f), which is (int g). 
    System.out.print(g);
    
    }
    
